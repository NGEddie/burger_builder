{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "react-app", "plugin:prettier/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier", "jsx-a11y"],
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "jsx-a11y/href-no-hash": "off",
    "no-console": "off"
  }
}
